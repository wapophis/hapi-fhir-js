{
  "name": "hapi-fhir",
  "version": "1.0.0",
  "description": "ECMA MODULE LIBRARY FOR FHIR IN THE CLIENT",
  "keywords": [
    "fhir",
    "hapi",
    "health",
    "interoperability"
  ],
  "author": "Luis Martinez (@Wapophis)",
  "homepage": "https://github.com/wapophis/hapi-fhir-js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wapophis/hapi-fhir-js.git"
  },
  "module": ".esm.js",
  "browser":".umd.js",
  "main": ".cjs.js",

  "scripts": {
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "testdebug": "./node_modules/.bin/mocha --inspect-brk --require @babel/register",
    "start": "nodemon --exec babel-node ./src/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "debug": "node --inspect-brk ./dist/index.js",
    "dev": "rollup -c -w"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/wapophis/hapi-fhir-js/issues"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "src/*"
  ]
}
